{
  "name": "express-query-builder",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/express-query-builder/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/express-query-builder",
        "main": "packages/express-query-builder/src/index.ts",
        "tsConfig": "packages/express-query-builder/tsconfig.lib.json",
        "assets": ["packages/express-query-builder/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs express-query-builder {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/express-query-builder/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/express-query-builder/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
